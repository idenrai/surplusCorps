{
  "name": "surpluscorps",
  "version": "1.0.0",
  "description": "react frontend template for Hwajung Surplus Corps",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --development -d --progress --colors --display-error-details",
    "staging": "node_modules/.bin/webpack --staging -p --progress --colors --display-error-details",
    "build": "node_modules/.bin/webpack --production -p --progress --colors --display-error-details",
    "test": "BABEL_JEST_STAGE=0 jest",
    "lint": "eslint src/**"
  },
  "author": "Youngjae Kwon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idenrai/surplusCorps.git"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
